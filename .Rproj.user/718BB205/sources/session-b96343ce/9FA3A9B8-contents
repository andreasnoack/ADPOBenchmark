library(Metrics)
CalcRMSE <- function(wd){
  data <- read.table(file.path(wd,"Run1Preds.dat"),skip = 1, header = TRUE) %>%
    filter(TIME > 0) %>%
    mutate(IPRED = log(IPRED), DV = log(DV))
  # log to get proportional

  RMSE <- rmse(data$DV, data$IPRED)
  MAE <- mae(data$DV, data$IPRED)
  retrun <- list(
    RMSE = RMSE,
    MAE = MAE
  )
}
